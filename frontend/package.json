{
  "name": "party-members-management",
  "description": "نظام شامل لإدارة بيانات أعضاء الحزب مع واجهة مستخدم حديثة ومتجاوبة",
  "version": "1.0.0",
  "type": "module",
  "author": "فريق تطوير نظام إدارة أعضاء الحزب",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amrshaker000/warraq.git"
  },
  "keywords": [
    "react",
    "typescript",
    "redux",
    "party-management",
    "member-management",
    "dashboard",
    "arabic",
    "rtl"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:dist": "rimraf dist",
    "analyze": "npx vite-bundle-analyzer dist",
    "test": "echo \"No tests specified yet\" && exit 0",
    "prepare": "husky install || true",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "@types/cors": "^2.8.19",
    "@types/react-datepicker": "^6.2.0",
    "axios": "^1.12.2",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "d3-scale": "^4.0.2",
    "date-fns": "^3.6.0",
    "framer-motion": "^12.23.22",
    "i18next": "^25.5.2",
    "lucide-react": "^0.544.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^16.0.0",
    "react-is": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.9.2",
    "react-simple-maps": "^3.0.0",
    "recharts": "^3.2.1",
    "sqlite3": "^5.1.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^24.6.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-simple-maps": "^3.0.6",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^0.0.0-18-20241205",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^5.4.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
